<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="100.0" prefWidth="250.0" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" fx:controller="plannerapp.view.CountryController">
    <stylesheets>
        <URL value="@forms.css"/>
    </stylesheets>
    <children>
      <Label fx:id="addCountryLabel" layoutX="112.0" layoutY="2.0" text="Country" />
      <Label fx:id="countryLabel" layoutX="20.0" layoutY="24.0" text="Country:" />
      <TextField fx:id="CountryTextField" layoutX="75.0" layoutY="20.0" />
      <Button fx:id="saveButton"  layoutX="50.0" layoutY="60.0" mnemonicParsing="false" onAction="#save" text="Save" />  
      <Button fx:id="cancelButton"  layoutX="125.0" layoutY="60.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />  
    </children>
</AnchorPane>
